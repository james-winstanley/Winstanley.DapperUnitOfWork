<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Authors>James Winstanley</Authors>
		<Company></Company>
		<PackageIcon>ui-avatars.com-jw-jira.png</PackageIcon>
		<PackageIconUrl />
		<RepositoryUrl>https://github.com/james-winstanley/Winstanley.UnitOfWork</RepositoryUrl>
		<PackageTags>James Winstanley, Database, Unit of Work</PackageTags>
		<Description>Common base library for repository + unit of work pattern.</Description>
		<Copyright>James Winstanley</Copyright>
		<Version>1.0.0</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>	  
