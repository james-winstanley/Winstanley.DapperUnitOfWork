		<PackageReference Include="Dapper.FastCrud.Concurrency" Version="1.0.0" />
		<PackageReference Include="Microsoft.Data.SqlClient" Version="3.0.1" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\..\..\Assets\Icons\ui-avatars.com-jw-jira.png">
	    <Pack>True</Pack>
	    <PackagePath></PackagePath>
	  </None>
	</ItemGroup>
